{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "lib": ["esnext"],
      "module": "commonjs",
      "moduleResolution": "node",
      "noEmit": false,
      "noFallthroughCasesInSwitch": true,
      "outDir": "../../build/api",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "target": "esnext"
    },
    "include": ["src"]
  }
  